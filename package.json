{
  "name": "ai-agent-flow",
  "version": "1.0.0",
  "description": "A Node.js framework for AI agent workflows",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./nodes/action": {
      "import": "./dist/esm/nodes/action.js",
      "require": "./dist/cjs/nodes/action.js",
      "types": "./dist/types/nodes/action.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js",
      "types": "./dist/types/types.d.ts"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "test": "jest",
    "docs:build": "typedoc",
    "docs:publish": "npm run docs:build && gh-pages -d docs",
    "prepare": "husky install",
    "lint": "eslint src tests examples --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "ai",
    "agent",
    "workflow",
    "typescript",
    "node"
  ],
  "author": "Rajesh Dhiman <rajesh@eunix.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EunixTech/ai-agent-flow.git"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "openai": "^4.95.1",
    "prom-client": "^14.2.0",
    "redis": "^4.6.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@typhonjs-typedoc/typedoc-theme-dmt": "^0.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.27.3",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
