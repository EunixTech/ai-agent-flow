{
  "name": "ai-agent-flow",
  "version": "1.0.0",
  "description": "A Node.js framework for AI agent workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "test": "jest",
    "docs:build": "typedoc",
    "docs:publish": "npm run docs:build && gh-pages -d docs",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "ai",
    "agent",
    "workflow",
    "typescript",
    "node"
  ],
  "author": "Rajesh Dhiman <your-email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-agent-flow.git"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "openai": "^4.0.0",
    "prom-client": "^14.2.0",
    "redis": "^4.6.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/jest": "^29.5.12",
    "@typhonjs-typedoc/typedoc-theme-dmt": "^0.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.27.3",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
